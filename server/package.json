{
  "name": "users-service",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "typeorm:js": "node ./node_modules/typeorm/cli.js -d dist/db/data-source.js",
    "migration:run": "npm run typeorm:js -- migration:run",
    "migration:revert": "npm run typeorm:js -- migration:revert"
  },
  "dependencies": {
    "argon2": "^0.40.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.30",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.2",
    "typescript": "^5.6.3"
  }
}
